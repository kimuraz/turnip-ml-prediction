{
  "name": "turnip-ml-prediction",
  "version": "1.0.0",
  "description": "This is an experiment to predict turnip prices from Animal Crossing New Horizons",
  "author": "Kimura",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "format": "prettier \"./layout/*.vue\" \"./components/*.vue\" \"./pages/*.vue\" --write",
    "format:ci": "prettier \"./layout/*.vue\" \"./components/*.vue\" \"./pages/*.vue\" --check",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest"
  },
  "dependencies": {
    "@hapi/hapi": "^18.3.1",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/hapi": "^2.2.1",
    "@tensorflow/tfjs": "1.0.0",
    "@tensorflow/tfjs-vis": "1.0.2",
    "ant-design-vue": "^1.1.10",
    "bcrypt": "^5.0.0",
    "cross-env": "^5.2.0",
    "hapi-auth-jwt2": "^10.1.0",
    "jsonwebtoken": "^8.5.1",
    "nuxt": "^2.0.0",
    "pg": "^8.2.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.9",
    "vuex-persist": "^2.2.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^24.1.0",
    "node-sass": "^4.14.1",
    "nodemon": "^1.18.9",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.2",
    "sequelize-cli": "^5.5.1",
    "vue-jest": "^4.0.0-0"
  }
}
